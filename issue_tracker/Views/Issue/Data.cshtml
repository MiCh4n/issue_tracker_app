@model IEnumerable<issue_tracker.Models.Issue>

@{
    ViewData["Title"] = "Dashboard";
}
<h1>@ViewData["Data"]</h1>
<div class="container">
        <div class="row">
            <div class="col-sm">Title</div>
            <div class="col-sm">Description</div>
            <div class="col-sm">Priority</div>
            <div class="col-sm">Author</div>
            <div class="col-sm">Date</div>
            <div class="col-sm">Phase</div>
        </div>
    </div>
@foreach (var item in Model) {
    
    <div class="container-fluid">
        <div class="row">
            @Html.DisplayFor(modelItem => item.Title)<br>
            @Html.DisplayFor(modelItem => item.Author)<br>
            @Html.DisplayFor(modelItem => item.AddDate)<br>
            @Html.DisplayFor(modelItem => item.Priority)<br>
            @Html.DisplayFor(modelItem => item.Phase)<br>
            @Html.DisplayFor(modelItem => item.Description)<br><br>
        </div></div>
}
